{
	"firestore": {
		"database": "(default)",
		"location": "nam5",
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"disallowLegacyRuntimeConfig": true,
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log",
				"*.local"
			],
			"predeploy": [
				"npm --prefix \"$RESOURCE_DIR\" run lint",
				"npm --prefix \"$RESOURCE_DIR\" run build"
			]
		}
	],
	"storage": { "rules": "storage.rules" },
	"emulators": {
		"ui": {
			"enabled": true,
			"port": 21200
		},
		"auth": { "port": 21201 },
		"functions": { "port": 21202 },
		"firestore": { "port": 21203 },
		"storage": { "port": 21204 },
		"hub": {
			"port": 21207
		},
		"logging": {
			"port": 21208
		},
		"singleProjectMode": true
	},
	"remoteconfig": { "template": "remoteconfig.template.json" },
	"flutter": {
		"platforms": {
			"android": {
				"default": {
					"projectId": "flutterplatformkit",
					"appId": "1:366512631366:android:dcce44f8481bea08d22447",
					"fileOutput": "android/app/google-services.json"
				}
			},
			"ios": {
				"default": {
					"projectId": "flutterplatformkit",
					"appId": "1:366512631366:ios:a2bda309bea09ad8d22447",
					"uploadDebugSymbols": true,
					"fileOutput": "ios/Runner/GoogleService-Info.plist"
				}
			},
			"dart": {
				"lib/firebase_options.dart": {
					"projectId": "flutterplatformkit",
					"configurations": {
						"android": "1:366512631366:android:dcce44f8481bea08d22447",
						"ios": "1:366512631366:ios:a2bda309bea09ad8d22447",
						"web": "1:366512631366:web:607ff3f7e71e8347d22447"
					}
				}
			}
		}
	}
}
